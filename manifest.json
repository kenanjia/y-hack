{
  "manifest_version": 2,

  "name": "Bookmark+",
  "description": "This is a bookmark system that doesn't suck.",
  "version": "0.1",

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "browser_action": {
    "default_title": "This is a test title"
  },

  "background": {
    "scripts": ["menu.js"]
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ]
}
